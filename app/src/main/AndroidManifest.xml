<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lh.hermes">

    <application
        android:name=".application.app"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".service.HermesService"
            android:process=":hermes1" />

        <service android:name="com.library.hermes.HermesService$HermesService0" />

        <service
            android:name=".service.ThreadService"
            android:process=":hermes2" />
        <service
            android:name=".service.SupperService"
            android:process=":hermes3" />
    </application>

</manifest>